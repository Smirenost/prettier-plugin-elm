{
  "name": "prettier-plugin-elm",
  "version": "0.5.0",
  "license": "BSD-3-Clause",
  "repository": "https://github.com/gicentre/prettier-plugin-elm",
  "description": "Adds Elm language support to Prettier via elm-format",
  "scripts": {
    "build": "tsc --project .",
    "clean": "rimraf cache coverage dist",
    "format": "prettier --write {*,{azure-pipelines,src}/**/*}.{md,js,json,ts,yml}",
    "lint": "tsc --project . --noEmit && tslint --project . && prettier --check {*,{azure-pipelines,src}/**/*}.{md,js,json,ts,yml}",
    "test": "yarn clean && yarn build && jest",
    "prepublishOnly": "yarn lint && yarn test"
  },
  "main": "dist",
  "keywords": [
    "elm",
    "elm-format",
    "gicentre",
    "prettier",
    "prettier-plugin",
    "typescript"
  ],
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "elm-format": "^0.8.1",
    "execa": "^2.0.3",
    "make-dir": "^3.0.0",
    "object-hash": "^1.3.1",
    "serialize-error": "^4.1.0",
    "temp-dir": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/object-hash": "^1.3.0",
    "@types/prettier": "^1.18.0",
    "@types/rimraf": "^2.0.2",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "sleep-promise": "^8.0.1",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  }
}
